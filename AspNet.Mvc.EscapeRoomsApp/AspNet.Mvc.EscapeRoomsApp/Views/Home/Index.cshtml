@model AspNet.Mvc.EscapeRoomsApp.Models.ViewModals.Products.ProductListViewModel

@{
    ViewData["Title"] = " The Ultimate Escape Rooms Experience";
}
<a asp-controller="Order" asp-action="CreateProduct" class="btn btn-success">Click here to Customize your own</a>
<h1>List of our most popular Escape Rooms:</h1>

@if (ViewBag.Message != null)
{
    <div class="alert alert-success">
        @ViewBag.Message
    </div>
}


<div class="row">

    <div class="col-md-12">
        <ol>
            @foreach (AspNet.Mvc.EscapeRoomsApp.Models.ViewModals.Products.ProductViewModel product in Model.EscapeRooms)
            {
                
                <li><h4>Escape Room:@product.Name</h4></li>
                <p>Price:@product.Price $</p>
                <img src="data:image/jpeg;base64," />
                <br />
                <a asp-area="" asp-controller="Order" asp-action="Index" asp-route-id="@product.Id"><button class="btn btn-success">More Details for the EscapeRoom</button></a>
            }
        </ol>

    </div>
</div>







